{
  "name": "gitspace",
  "version": "0.0.1",
  "description": "A WebSocket hub which facilitates collaboration in 3D space, while archiving activity to a Git repo",
  "main": "build/gitspace.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "usage"
  },
  "devDependencies": {
    "apage": "latest",
    "coffee-script": "latest",
    "nodemon": "latest"
  },
  "dependencies": {
  },
  "keywords": [
    "WebSocket",
    "hub",
    "3D",
    "Git",
    "Collaborate",
    "Collaborative",
    "Creative",
    "CoffeeScript",
    "Literate CoffeeScript"
  ],
  "scripts": {
    "hi": "open .; open index.html; open test/run-test.html; subl .",
    "build": "node oopish/bin/oopish-build",
    "doc": "node_modules/apage/bin/apage -i README.md doc/*.md -o index.html -p \"`cat node_modules/apage/build/plugin/*.js`\"",
    "test": "node test/run-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richplastow/gitspace.git"
  },
  "author": "Rich Plastow <rich@richplastow.com> (http://richplastow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richplastow/gitspace/issues"
  },
  "homepage": "http://gitspace.richplastow.com/"
}
